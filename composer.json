{
    "name": "opsi/laravel-offline",
    "description": "Production-ready offline-first functionality for Laravel applications with intelligent caching, background sync, and request queueing.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "offline",
        "pwa",
        "progressive web app",
        "offline-first",
        "service worker",
        "cache strategies",
        "background sync",
        "laravel package"
    ],
    "authors": [
        {
            "name": "o-psi",
            "homepage": "https://github.com/o-psi"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "illuminate/support": "^8.12|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.5|^10.0"
    },
    "autoload-dev": {
        "psr-4": {
            "EragLaravelPwa\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/pint"
    },
    "autoload": {
        "psr-4": {
            "Opsi\\LaravelOffline\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Opsi\\LaravelOffline\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Opsi\\LaravelOffline\\OfflineServiceProvider"
            ],
            "aliases": {
                "Offline": "Opsi\\LaravelOffline\\Facades\\Offline"
            }
        }
    }
}
